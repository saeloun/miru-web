{
  "settings": {
    "app_name": "Miru",
    "app_version": "1.0.0",
    "server_url": "http://localhost:3000"
  },
  "tabs": [
    {
      "title": "Time",
      "path": "/time-tracking",
      "icon": "clock"
    },
    {
      "title": "Clients",
      "path": "/clients",
      "icon": "users"
    },
    {
      "title": "Projects",
      "path": "/projects",
      "icon": "folder"
    },
    {
      "title": "Reports",
      "path": "/reports",
      "icon": "chart"
    },
    {
      "title": "More",
      "path": "/settings/profile",
      "icon": "menu"
    }
  ],
  "rules": [
    {
      "patterns": [".*"],
      "properties": {
        "presentation": "default",
        "pull_to_refresh": true
      }
    },
    {
      "patterns": ["/sign_in", "/sign_up"],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh": false
      }
    },
    {
      "patterns": ["/settings/.*"],
      "properties": {
        "presentation": "push",
        "pull_to_refresh": false
      }
    }
  ]
}